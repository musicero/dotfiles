set $mod Mod4
set $wsz "Z"
set $wsx "X"
set $wsc "C"
set $wsa "A"
set $wsy "Y"
set $wsu "U"
set $wsi "I"
set $wso "O"
set $wsp "P"

set $wszk "Z"
set $wsxk "X"
set $wsck "C"
set $wsak "A"
set $wsyk "Y"
set $wsuk "U"
set $wsik "I"
set $wsok "O"
set $wspk "P"

set $ws11 "1"
set $ws12 "2"
set $ws13 "3"
set $ws14 "4"
set $ws15 "5"
set $ws16 "6"

bindsym $mod+$wszk	workspace $wsz
bindsym $mod+$wsxk	workspace $wsx
bindsym $mod+$wsck   workspace $wsc
bindsym $mod+$wsak   workspace $wsa
bindsym $mod+$wsyk   workspace $wsy
bindsym $mod+$wsuk   workspace $wsu
bindsym $mod+$wsik	workspace $wsi
bindsym $mod+$wsok	workspace $wso
bindsym $mod+$wspk	workspace $wsp
bindsym $mod+$ws11  workspace number $ws11
bindsym $mod+$ws12  workspace number $ws12
bindsym $mod+$ws13  workspace number $ws13
bindsym $mod+$ws14  workspace number $ws14
bindsym $mod+$ws15	workspace number $ws15
bindsym $mod+$ws16	workspace number $ws16

bindsym $mod+Shift+$wszk move container to workspace $wsz
bindsym $mod+Shift+$wsxk move container to workspace $wsx
bindsym $mod+Shift+$wsck move container to workspace $wsc
bindsym $mod+Shift+$wsak move container to workspace $wsa
bindsym $mod+Shift+$wsyk move container to workspace $wsy
bindsym $mod+Shift+$wsuk move container to workspace $wsu
bindsym $mod+Shift+$wsik move container to workspace $wsi
bindsym $mod+Shift+$wsok move container to workspace $wso
bindsym $mod+Shift+$wspk move container to workspace $wsp
bindsym $mod+Shift+$ws11 move container to workspace number $ws11
bindsym $mod+Shift+$ws12 move container to workspace number $ws12
bindsym $mod+Shift+$ws13 move container to workspace number $ws13
bindsym $mod+Shift+$ws14 move container to workspace number $ws14
bindsym $mod+Shift+$ws15 move container to workspace number $ws15
bindsym $mod+Shift+$ws16 move container to workspace number $ws16

for_window [title="Microsoft Teams Notification"] floating enable
for_window [title="Microsoft Teams Notification"] sticky enable
for_window [title="feh"] floating enable
for_window [title="SFML"] floating enable
